{
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": false,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false,
    "parser": "babel",
    "overrides": [
      {
        "files": ["*.mjs", "*.cjs", "*.js"],
        "options": {
          "parser": "babel"
        }
      },
  
      {
        "files": ["*.mts", "*.cts", "*.ts"],
        "options": {
          "parser": "babel-ts"
        }
      },
  
      {
        "files": ["*.json", "*.jsonc", "*.json5"],
        "options": {
          "parser": "json"
        }
      }
    ]
  }